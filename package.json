{
  "name": "ai-proxy-server",
  "version": "1.0.0",
  "description": "Simple backend proxy API to handle requests between your macOS SwiftUI app and the Mistral AI API (or another third-party AI API)",
  "author": "Pierre-Henry Soria",
  "homepage": "https://github.com/SucceedAI",
  "private": true,
  "scripts": {
    "start": "ts-node src/server.ts",
    "build": "tsc",
    "dev": "nodemon src/server.ts",
    "prettier:check": "./node_modules/.bin/prettier --check \"src/**/*.{ts,js,json}\" \"!package.json\"",
    "prettier:fix": "./node_modules/.bin/prettier --write \"**/*.{ts,js,json}\" \"!package.json\""

  },
  "dependencies": {
    "@mistralai/mistralai": "^0.1.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.2.0",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "openai": "^4.28.4",
    "pino": "^8.19.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
